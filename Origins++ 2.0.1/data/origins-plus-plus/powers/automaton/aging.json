{
	"type":"origins:multiple",
	"hidden":true,
	"exhaust":{
		"type":"origins:action_over_time",
		"interval":1,
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"resource change @s origins-plus-plus:automaton/waterbar -1"
				},
				{
					"type":"origins:execute_command",
					"command":"resource change @s origins-plus-plus:automaton/firebar -1"
				}
			]
		},
		"condition":{
			"type":"origins:or",
			"conditions":[
				{
					"type":"origins:sprinting"
				},
				{
					"type":"origins:swimming"
				},
				{
					"type":"origins:on_block",
					"inverted":true
				}
			]
		}
	},
	"decrease":{
		"type":"origins:action_over_time",
		"interval":10,
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"resource change @s origins-plus-plus:automaton/waterbar -1"
				},
				{
					"type":"origins:execute_command",
					"command":"resource change @s origins-plus-plus:automaton/firebar -1"
				}
			]
		}
	},
	"empty":{
		"type":"origins:action_over_time",
		"interval":400,
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"resource change @s origins-plus-plus:automaton/waterbar -2"
				},
				{
					"type":"origins:execute_command",
					"command":"resource change @s origins-plus-plus:automaton/firebar -2"
				},
				{
					"type":"origins:execute_command",
					"command":"particle minecraft:smoke ~ ~ ~ 0.5 1 0.5 0 5"
				},
				{
					"type":"origins:execute_command",
					"command":"playsound minecraft:entity.cat.hiss voice @a ~ ~ ~ 0.35 0.5"
				}
			]
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"resource":"origins-plus-plus:automaton/firebar",
					"comparison":">",
					"compare_to":0
				},
				{
					"type":"origins:resource",
					"resource":"origins-plus-plus:automaton/waterbar",
					"comparison":">",
					"compare_to":0
				}
			]
		}
	}
}