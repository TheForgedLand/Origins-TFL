{
	"hidden":true,
	"type":"origins:multiple",
	"callback":{
		"type":"origins:action_on_callback",
		"entity_action_lost":{
			"type":"origins:execute_command",
			"command":"team remove light-mage"
		}
	},
	"add":{
		"type":"origins:active_self",
		"key":"key.origins.primary_active",
		"cooldown":100,
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"team join light-mage @a[distance=1..6]"
				},
				{
					"type":"origins:execute_command",
					"command":"playsound minecraft:block.note_block.flute player @a[distance=..6] ~ ~ ~ 100 2"
				},
				{
					"type":"origins:equipped_item_action",
					"equipment_slot":"offhand",
					"action":{
						"type":"origins:damage",
						"amount":20,
						"ignore_unbreaking":false
					}
				}
			]
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:sneaking"
				},
				{
					"type":"origins:equipped_item",
					"equipment_slot":"offhand",
					"item_condition":{
						"type":"origins:ingredient",
						"ingredient":{
							"item":"minecraft:wooden_sword"
						}
					}
				}
			]
		}
	},
	"remove":{
		"type":"origins:active_self",
		"key":"key.origins.secondary_active",
		"cooldown":100,
		"entity_action":{
			"type":"origins:and",
			"actions":[
				{
					"type":"origins:execute_command",
					"command":"team leave @a[distance=1..6]"
				},
				{
					"type":"origins:execute_command",
					"command":"playsound minecraft:block.note_block.flute player @a[distance=..6] ~ ~ ~ 100 0.1"
				},
				{
					"type":"origins:equipped_item_action",
					"equipment_slot":"offhand",
					"action":{
						"type":"origins:damage",
						"amount":20,
						"ignore_unbreaking":false
					}
				}
			]
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:sneaking"
				},
				{
					"type":"origins:equipped_item",
					"equipment_slot":"offhand",
					"item_condition":{
						"type":"origins:ingredient",
						"ingredient":{
							"item":"minecraft:wooden_sword"
						}
					}
				}
			]
		}
	}
}